<p id="notice"><%= notice %></p>

<h1>Listing Seos</h1>

<table>
  <thead>
    <tr>
      <th>Tipo</th>
      <th>Parole</th>
      <th>Importo</th>
      <th>Attivazione</th>
      <th>Mensilita</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @seos.each do |seo| %>
      <tr>
        <td><%= seo.tipo %></td>
        <td><%= seo.parole %></td>
        <td><%= seo.importo %></td>
        <td><%= seo.attivazione %></td>
        <td><%= seo.mensilita %></td>
        <td><%= link_to 'Show', seo %></td>
        <td><%= link_to 'Edit', edit_seo_path(seo) %></td>
        <td><%= link_to 'Destroy', seo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Seo', new_seo_path %>
